SUPABASE_URL=your-project-url
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
JWT_SECRET=your-jwt-secret

# ⚠️ TEST ENVIRONMENT VARIABLES - REMOVE BEFORE PRODUCTION ⚠️
# These variables enable test-only authentication bypass
# CRITICAL: Set to 'false' or remove entirely in production
TEST_MODE=false
ENABLE_TEST_BYPASS=false

# MOCK DATABASE SETTING - Set to 'true' to use in-memory mock database
# ⚠️ WARNING: Mock database is for testing only - all data resets on server restart
# CRITICAL: Set to 'false' or remove entirely in production
USE_MOCK_DB=false

# Production safety check - should always be 'production' in live environment
NODE_ENV=development